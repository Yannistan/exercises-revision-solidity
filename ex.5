pragma solidity ^0.6.0;


contract mapToMap {
  
   mapping (string => mapping (string => string)) public _earth;
   
     constructor() public{    
       _earth["europe"]["france"] = "paris";
       _earth["amerique du sud"]["argentine"] = "buenos aires";
    } 
    
    function earth(string memory continent, string memory country) public returns (string memory) {
       
        return _earth[continent][country];
    }
    
    
}


